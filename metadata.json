{
  "name": "ffrank-mgmtgraph",
  "version": "0.6.1",
  "author": "Felix Frank",
  "summary": "Puppet module that adds the mgmtgraph face",
  "license": "Apache-2.0",
  "source": "https://github.com/ffrank/puppet-mgmtgraph",
  "issues_url": "https://github.com/ffrank/puppet-mgmtgraph/issues",
  "project_page": "https://github.com/ffrank/puppet-mgmtgraph",
  "dependencies": [
    { "name": "ffrank-yamlresource", "version_requirement": ">= 0.2.0" }
  ],
  "tags": [ "compiler", "catalog", "mgmt" ],
  "operatingsystem_support": [
    {
    "operatingsystem":"RedHat",
    "operatingsystemrelease":[ "6.0", "7.0" ]
    },
    {
    "operatingsystem":"Debian",
    "operatingsystemrelease":[ "8.0", "9.0" ]
    },
    {
    "operatingsystem":"Ubuntu",
    "operatingsystemrelease":[ "12.04", "14.04", "16.04", "18.04" ]
    }
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">= 5.0.0 < 7.0.0"
    }
  ]
}
